    "contentHtml": "<script>\n  \nwidget._gridRoot = null;\nwidget._dataGrid = null;\nwidget.componentReadyHandler = function(componentId){\n  if(!widget.isBindingSource()){\n    document.getElementById(componentId).setDataType(\"json\");\n    document.getElementById(componentId).setDataURLEx(BIX5.getAssetsPath() + \"Datasources/data2.json\");\n  }\n      \n  widget._gridRoot = widget.getComponent().getRoot();\n  var dataCompleteHandler = function(event) {\n    widget._gridRoot.removeEventListener(\"dataComplete\", dataCompleteHandler);\n    widget._dataGrid = widget._gridRoot.getDataGrid();\n\n    widget._dataGrid.addEventListener(\"itemClick\", widget._itemClickHandler);\n  }\n  widget._gridRoot.addEventListener(\"dataComplete\", dataCompleteHandler);\n  \n}\n\n\nwidget.selectItem = function(data){\n  var selectItemEvent = BIX5.dashboard.createEvent(\"selectItem\");\n  selectItemEvent.detail = {\n    data : JSON.parse(JSON.stringify(data))\n};\n  sendMsgToParent(data);\n }\n widget._itemClickHandler = function(event){\n  widget.selectItem(widget._gridRoot.getItemAt(event.rowIndex));\n}\n  \nfunction sendMsgToParent(msg){\n  window.parent.postMessage(msg, '*');\n}\n  \n \n \n \n\n\n</script>",
